/* pages/user/dealers/dealerDetail/dealerDetail.wxss */
page{
  background: #eeeeee
}
.dealers-title{
  width: 100%;
  height: 90rpx;
  background:#fea304;
  color: #fff;
  font-size: 28rpx;
  line-height: 90rpx;
  text-align: center
 
}
.dealers-title text{
 border-left: 6rpx solid #fff;
 padding-left: 20rpx;
 width: 90%;
 display: inline-block;
 line-height: 8rpx;
 height: 25rpx;
 text-align: left
}
.dealers-item{
  width: 100%;
  background: #fff;
  display: inline-block;
  padding-bottom: 20rpx;
  margin-bottom: 20rpx;
}
.dealers-item view{
  color: #999;
  font-size: 26rpx;
  padding-top: 25rpx;
  width: 90%;
  margin: 0 auto
}
.dealers-item view text{
  color: #333;
}
.dealers-item view.title text{
 line-height: 55rpx;
}
