/* pages/shop-cart/shop-cart.wxss */

page {
  min-height: 100%;
  background-color: #f5f5f5;
}

.container {
  display: block;
  min-height: 100%;
  background-color: #f5f5f5;
  padding-top: 18rpx;
}

.settle.no-select {
  background: #999;
}

.cart-top {
  width: 100%;
  text-align: center;
}

.cart-top view {
  font-size: 30rpx;
}

.cart-top>image {
  margin-top: 100rpx;
  width: 288rpx;
  height: 249rpx;
}

.cart-top-l {
  float: left;
  font-size: 28rpx;
  line-height: 102rpx;
  margin-left: 20rpx;
  color: #333;
}

.cart-manage {
  float: right;
  font-size: 28rpx;
  line-height: 102rpx;
  color: #ff9300;
  margin-right: 20rpx;
}

.main {
  width: 100%;
  background: #fff;
  position: relative;
}

.cart-null {
  width: 100%;
  height: 618rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.content-box {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 5;
}

.title-box {
  width: 100%;
  padding-top: 440rpx;
  text-align: center;
  font-size: 28rpx;
  color: #999;
  background-size: 100rpx auto;
  margin-bottom: 50rpx;
}

.to-index-btn {
  width: 258rpx;
  height: 58rpx;
  background-color: #cd9b01;
  font-size: 24rpx;
  line-height: 58rpx;
  text-align: center;
  color: #fff;
  border-radius: 8rpx;
  margin-left: 245rpx;
}

.goodsList {
  width: 100%;
  height: auto;
}

.goods-box {
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-bottom: 18rpx;
  background-color: #fff;
}

.goods-content {
  position: relative;
  width: 100%;
  justify-content: space-between;
  height: 270rpx;
  background-color: #fff;
}

.goods-choose {
  height: 270rpx;
  float: left;
  width: 100%;
  position: absolute;
  z-index: 1000;
}

.goods-choose>view {
  height: 250rpx;
  z-index: 100;
}

/*checkbox 整体大小  */

checkbox {
  float: left;
  width: 34rpx;
  height: 34rpx;
}

/* checkbox-group.active checkbox{
    
    z-index: 
   
} */

/*checkbox 选项框大小  */

.good-left {
  width: 7%;
  height: 250rpx;
  float: left;
  position: relative;
}

.icon, .choose-all view {
  width: 40rpx;
  height: 40rpx;
  background: url(https://cdn.it120.cc/images/weappshop/gou.png) no-repeat;
  background-size: cover;
  float: left;
  margin-left: 15rpx;
  margin-right: 22rpx;
  position: absolute;
  top: 140rpx;
}

.choose-all view {
  z-index: 9999;
  position: static !important;
}

.icon.active, .choose-all view.active {
  /* background: url(https://cdn.it120.cc/images/weappshop/gou.png) no-repeat; */
  background: url(https://meiyankongjian.com/rejiangupload/yes.png) no-repeat 0 0;
  background-size: cover;
  margin-left: 15rpx;
  z-index: 10;
  margin-right: 22rpx;
  position: absolute;
  top: 140rpx;
}

/*checkbox选中后图标样式  */

.goods-img {
  float: left;
  width: 220rpx;
  height: 200rpx;
  overflow: hidden;
  margin-top: 28rpx;
}

.goods-img image {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}

.text-box {
  float: left;
  width: 428rpx;
  margin-left: 42rpx;
}

.goods-tit {
  display: inline-block;
  width: 330rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 26rpx;
  line-height: 40rpx;
  margin-top: 40rpx;
  color: #595959;
  margin-bottom: 30rpx;
}
.goods-tit.level{
   margin-bottom: 30rpx;
}
.goods-p {
  display: inline-block;
  width: 204rpx;
  height: 36rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 30rpx;
  line-height: 36rpx;
  margin-top: 30rpx;
  color: #cd9b01;
}

.num-box {
  position: absolute;
  right: 10rpx;
  z-index: 1999;
  bottom: 40rpx;
}

.num-less, .num-plus {
  height: 56rpx;
}

.num-less, .num-show, .num-plus {
  /* float: left; */
  width: 72rpx;
  text-align: center;
  line-height: 56rpx;
  margin-right: 4rpx;
  /* background-color: #f2f2f2; */
  color: #cd9b01;
  font-weight: bold;
}

.num-show {
  height: 90rpx;
  font-size: 26rpx;
}

.num-show input {
  height: 100%;
  color: #cd9b01;
}

.settle-box {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  border-top: 1px solid #dcdcdc;
  background-color: #fff;
}

.choose-all {
  width: 40rpx;
  height: 40rpx;
  float: left;
  margin-left: 22rpx;
  margin-top: 22rpx;
}

.settle-t {
  float: left;
  font-size: 28rpx;
  line-height: 88rpx;
  color: #333;
  margin-left: 36rpx;
}

.total {
  max-width: 270rpx;
  float: right;
  font-size: 28rpx;
  line-height: 88rpx;
  color: #cd9b01;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-right: 36rpx;
}

.settle {
  width: 250rpx;
  height: 88rpx;
  float: right;
  font-size: 30rpx;
  line-height: 88rpx;
  color: #fff;
  text-align: center;
  background-color: #cd9b01;
}

button {
  border-radius: 0;
}

button::after {
  border-radius: 0;
  border: none;
}

.list-top {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  font-size: 28rpx;
  align-items: center;
  background: #fff;
  border-bottom: 1px solid #b6b4b3;
}

.list-top .label {
  color: #000;
}

.list-top .edit-btn {
  color: #999;
  height: 100%;
}

.goodsList {
  width: 100%;
  margin-bottom: 100rpx;
}

.a-gooods {
  width: 100%;
  overflow: hidden;
}

.a-goods-conts {
  display: flex;
  justify-content: space-between;
  padding-left: 30rpx;
  width: 870rpx;
  box-sizing: border-box;
  transition: margin-left 0.2s ease-in-out;
}

.goods-info {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 0 30rpx 30rpx;
  flex: 93%;
  margin-left: 7%;
  box-sizing: border-box;
  position: relative;
  top: 0;
}

.goods-info .img-box {
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  margin-right: 20rpx;
  background-color: #d8d8d8;
}

.goods-info .text-box {
  width: 440rpx;
  position: relative;
}

.goods-info .text-box .goods-title {
  font-size: 28rpx;
  color: #000;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  padding: 10rpx 20rpx 5rpx 0;
}

.goods-info .text-box .goods-label {
  font-size: 26rpx;
  color: #999;
  height: 38rpx;
  line-height: 38rpx;
  margin: 8rpx 0 25rpx 0;
}

.goods-info .text-box .goods-price {
  font-size: 26rpx;
  color: #cd9b01;
}

.goods-info .text-box .buy-num {
  width: 164rpx;
  height: 48rpx;
  line-height: 48rpx;
  position: absolute;
  right: 30rpx;
  bottom: 0;
  display: flex;
  /*justify-content: space-between;*/
  font-size: 24rpx;
  text-align: center;
}

.goods-info .text-box .buy-num .jian-btn {
  width: 48rpx;
  height: 100%;
  border-left: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
  border-bottom-left-radius: 6rpx;
  border-top-left-radius: 6rpx;
}

.goods-info .text-box .buy-num .jian-btn.disabled {
  background-color: #f5f5f9;
  border-left: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  border-top: 1rpx solid #eee;
  color: #ccc;
}

.goods-info .text-box .buy-num .jia-btn {
  width: 48rpx;
  height: 100%;
  border-right: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
  border-bottom-right-radius: 6rpx;
  border-top-right-radius: 6rpx;
}

.goods-info .text-box .buy-num .jia-btn.disabled {
  background-color: #f5f5f9;
  border-right: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  border-top: 1rpx solid #eee;
  color: #ccc;
}

.goods-info .text-box .buy-num input {
  width: 68rpx;
  height: 48rpx;
  min-height: 48rpx;
  text-align: center;
  font-size: 24rpx;
  border: 1rpx solid #ccc;
}

.goods-info .img-box .img {
  width: 160rpx;
  height: 160rpx;
}

.a-goods-conts .delete-btn {
  width: 120rpx;
  line-height: 220rpx;
  text-align: center;
  background: #cd9b01;
  font-size: 24rpx;
  color: #fff;
}

.jiesuan-box {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 90rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  border-bottom: 1px solid #eee;
  background-color: #fff;
  z-index: 9999;
}

.jiesuan-box .to-pay-btn {
  width: 240rpx;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #cd9b01;
  font-size: 32rpx;
  color: #fff;
}

.jiesuan-box .to-pay-btn.no-select {
  background-color: #ccc;
}

.to-pay-btn.no-select {
  background-color: #ccc;
}

.jiesuan-box  .left-price {
  display: flex;
  width: 510rpx;
  justify-content: space-between;
  padding: 0 30rpx 0 0;
  font-size: 28rpx;
  box-sizing: border-box;
}

.jiesuan-box .all-selected {
  padding-left: 90rpx;
  color: #000;
  background: url(https://cdn.it120.cc/images/weappshop/gou.png) no-repeat 30rpx center;
  background-size: 40rpx auto;
}

.jiesuan-box .all-selected.active {
  background: url(https://cdn.it120.cc/images/weappshop/gou-red.png) no-repeat 30rpx center;
  background-size: 40rpx auto;
}

.jiesuan-box .total {
  color: #cd9b01;
}
